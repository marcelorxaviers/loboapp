<%= simple_form_for([@company, @contact]) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :first_name %>
    <%= f.input :last_name %>
    <%= f.input :email %>
    <%= f.input :company_name %>
    <%= f.input :job_title %>
    <%= f.input :phone %>
    <%= f.input :website %>
  </div>

  <div class="form-actions">
    <%= link_to :back, class: "btn" do %>
      Cancel
    <% end %>
    <%= f.button :submit, class: "btn btn-primary" %>
  </div>
<% end %>